def caesar_decrypt(ciphertext, key):
    decrypted_text = ""
    for char in ciphertext:
        if char.isalpha():
            shift = ord('A')  # Since the alphabet is uppercase only
            decrypted_text += chr((ord(char) - shift - key) % 26 + shift)
        else:
            decrypted_text += char
    return decrypted_text

# Example usage
ciphertext = ("SROKBDSVIKMMOZDDROWYDDYDRKDQYFOBXWOXDSCLOCDGRSMRQYFOBXCVOKCDKXNSCRYEVNVSUODYCOOS"
              "DKMDONEZDYWYBOBKZSNVIKXNCICDOWKDSMKVVIMKBBSONYEDSDPSXKVVIKWYEXDCDYDRSCGRSMRKVCYS"
              "LOVSOFODRKDQYFOBXWOXDSCLOCDGRSMRQYFOBXCXYDKDKVVKXNGROXWOXKBOZBOZKBONPYBSDDRKDGSV"
              "VLODROUSXNYPQYFOBXWOXDGRSMRDROIGSVVRKFOQYFOBXWOXDSCKDLOCDLEDKXOHZONSOXDLEDWYCDQY"
              "FOBXWOXDCKBOECEKVVIKXNKVVQYFOBXWOXDCKBOCYWODSWOCSXOHZONSOXDDROYLTOMDSYXCGRSMRRKF"
              "OLOOXLBYEQRDKQKSXCDKCDKXNSXQKBWIKXNDROIKBOWKXIKXNGOSQRDIKXNNOCOBFODYZBOFKSVWKIKV"
              "CYKDVKCDLOLBYEQRDKQKSXCDKCDKXNSXQQYFOBXWOXDDROCDKXNSXQKBWISCYXVIKXKBWYPDROCDKXNS"
              "XQQYFOBXWOXDDROQYFOBXWOXDSDCOVPGRSMRSCYXVIDROWYNOGRSMRDROZOYZVORKFOMRYCOXDYOHOME"
              "DODROSBGSVVSCOAEKVVIVSKLVODYLOKLECONKXNZOBFOBDONLOPYBODROZOYZVOMKXKMDDRBYEQRSDGS"
              "DXOCCDROZBOCOXDWOHSMKXGKBDROGYBUYPMYWZKBKDSFOVIKPOGSXNSFSNEKVCECSXQDROCDKXNSXQQY"
              "FOBXWOXDKCDROSBDYYVPYBSXDROYEDCODDROZOYZVOGYEVNXYDRKFOMY COXDONDYDRSCWOKCEBOMCOP"
              "SFOPYEBDRBOO")

key = 10  # 'K' corresponds to 10
print("Decrypted text:", caesar_decrypt(ciphertext, key))
